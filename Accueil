<script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&amp;render=explicit" async="" defer=""></script>

<script type="text/javascript" charset="utf-8">
  // app-grecacpcha-checkbot data-size data- theme
  // ---> globalsJs.php siteKey

  //  Grecapcha.ononloadCallback
  var renderCaptchaById;
  var onloadCallback = function(callback) {
    var recaptchas = document.querySelectorAll('.app-recaptcha');
    /* @todo @wws-admin Remove try/catch for prevent multiple instances?  */
    renderCaptchaById = function(id, theme) {
      try {
          widgetId = grecaptcha.render(id, {
            'sitekey' : '6LfaEqcUAAAAAIEGeAqlI36kypqW0fxsC4wq2nyZ',
            'size'    : 'normal',
            'theme'   : theme || 'light'
          });
          return widgetId;
      }
      catch (error) {
        //console.debug(error);
      }
    }
    for (i = 0; i < recaptchas.length; i++) {
      widgetId = renderCaptchaById(recaptchas[i].id);
      recaptchas[i].dataset.widgetid = widgetId;
    }
    if (typeof callback == "function") {
      callback()
    }
  }
</script>
<script>
    
    document.addEventListener('DOMContentLoaded', function () {
                    })
</script>

